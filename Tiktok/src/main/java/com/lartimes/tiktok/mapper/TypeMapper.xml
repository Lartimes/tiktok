<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lartimes.tiktok.mapper.TypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lartimes.tiktok.model.po.Type">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="open" property="open" />
        <result column="icon" property="icon" />
        <result column="sort" property="sort" />
        <result column="label_names" property="labelNames" />
        <result column="is_deleted" property="deleted" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_updated" property="gmtUpdated" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, description, open, icon, sort, label_names, is_deleted, gmt_created, gmt_updated
    </sql>

</mapper>
