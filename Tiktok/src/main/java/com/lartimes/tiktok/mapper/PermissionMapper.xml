<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lartimes.tiktok.mapper.PermissionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lartimes.tiktok.model.user.Permission">
        <id column="id" property="id" />
        <result column="p_id" property="pId" />
        <result column="path" property="path" />
        <result column="href" property="href" />
        <result column="icon" property="icon" />
        <result column="name" property="name" />
        <result column="is_menu" property="isMenu" />
        <result column="target" property="target" />
        <result column="sort" property="sort" />
        <result column="state" property="state" />
        <result column="is_deleted" property="isDeleted" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_updated" property="gmtUpdated" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, p_id, path, href, icon, name, is_menu, target, sort, state, is_deleted, gmt_created, gmt_updated
    </sql>

</mapper>
