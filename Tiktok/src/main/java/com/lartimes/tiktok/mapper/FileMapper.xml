<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lartimes.tiktok.mapper.FileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lartimes.tiktok.model.video.File">
        <id column="id" property="id" />
        <result column="file_key" property="fileKey" />
        <result column="format" property="format" />
        <result column="type" property="type" />
        <result column="duration" property="duration" />
        <result column="size" property="size" />
        <result column="user_id" property="userId" />
        <result column="is_deleted" property="isDeleted" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_updated" property="gmtUpdated" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_key, format, type, duration, size, user_id, is_deleted, gmt_created, gmt_updated
    </sql>

</mapper>
